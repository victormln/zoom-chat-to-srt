{
    "name": "victormln/zoom-chat-to-srt",
    "description": "Convert zoom chat to valid .srt file",
    "require": {
        "php": ">=8.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Victormln\\ZoomChatToSrt\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Victormln\\ZoomChatToSrt\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "victormln",
            "email": "victor.molinaf@gmail.com"
        }
    ]
}
